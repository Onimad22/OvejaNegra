@model IEnumerable<OvejaNegra.Data.Entities.Sueldo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jornal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Bono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pago)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jornal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pago)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
